{
  "name": "eew4reso",
  "version": "1.0.0",
  "description": "EEW (Emergency Earthquake Warning) parser and WebSocket server for Resonite",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/examples/parser-example.ts",
    "dev:misskey": "ts-node src/examples/misskey-posting-example.ts",
    "dev:format": "ts-node src/examples/format-demo.ts",
    "dev:server": "ts-node src/index.ts",
    "dev:test-client": "ts-node src/examples/test-client.ts",
